{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 1,
    "react/react-in-jsx-scope": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true
},
  "parserOptions": {
    "ecmaVersion": 2020
  }

}
